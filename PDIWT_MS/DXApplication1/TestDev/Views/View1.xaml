<UserControl
    x:Class="TestDev.Views.View1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ViewModels="clr-namespace:TestDev.ViewModels"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="300"
    DataContext="{dxmvvm:ViewModelSource Type={x:Type ViewModels:ViewModel1}}"
    mc:Ignorable="d">
    <Grid>
        <dxc:ChartControl>
            <dxc:ChartControl.Titles>
                <dxc:Title HorizontalAlignment="Center" Content="Test Radar Chart" />
            </dxc:ChartControl.Titles>
            <dxc:RadarDiagram2D ShapeStyle="Circle">
                <dxc:RadarAreaSeries2D>
                    <dxc:SeriesPoint Argument="1" Value="5" />
                    <dxc:SeriesPoint Argument="2" Value="7" />
                    <dxc:SeriesPoint Argument="3" Value="4" />
                    <dxc:SeriesPoint Argument="4" Value="6" />
                    <dxc:SeriesPoint Argument="5" Value="6" />
                </dxc:RadarAreaSeries2D>
            </dxc:RadarDiagram2D>
        </dxc:ChartControl>
    </Grid>
</UserControl>
