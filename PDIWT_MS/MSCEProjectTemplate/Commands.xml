<?xml version="1.0" encoding="utf-8" ?>
<KeyinTree xmlns="http://www.bentley.com/schemas/1.0/MicroStation/AddIn/KeyinTree.xsd">

  <RootKeyinTable ID="root">
    <Keyword SubtableRef="$safeprojectname$" CommandClass="MacroCommand" CommandWord="$safeprojectname$">
      <Options Required="true" />
    </Keyword>
  </RootKeyinTable>

  <SubKeyinTables>
    <KeyinTable ID="$safeprojectname$">
      <Keyword CommandWord="Command"></Keyword>
    </KeyinTable>
  </SubKeyinTables>

  <KeyinHandlers>
    <KeyinHandler Keyin="$safeprojectname$ Command" Function="$safeprojectname$.KeyinCommands.Command"/>
  </KeyinHandlers>
</KeyinTree>